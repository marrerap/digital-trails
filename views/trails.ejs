<!DOCTYPE html>
<html lang="en">

<%-include('partials/head') %>

  <body class="body">
    <div class="row justify-content-end" style="margin-bottom: 10px;">
        <div class="col-1 " style="padding: 0px;">

          <a class="logout"  href="/logout">Logout</a>
      
        </div>
      </div>
    <%-include('partials/nav') %>
      
      
      <div class="row">
        <% trails.forEach((trail)=> { %>

          <div class="col-sm-4 " style="margin-top: 1rem;">

            <div class="cards justify-content-center" style="width: 18rem;">


              <div class="card-body">

                <h5 class="card-title">
                  <%=trail.trailName %>
                </h5>
                <h6 class=" mb-2 text-muted">Card subtitle</h6>
                <p class="card-text">
                  <%=trail.description %>
                </p>
                <a href="#" class="cardLink">Card link</a>

              </div>

            </div>
          </div>
          <% })%>
      </div>






  </body>

</html>